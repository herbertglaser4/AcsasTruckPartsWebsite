        <div class="checkout-area section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <!-- cpost listing start -->
                        <div class="client-address">
                            <!-- section title start -->
                            <div class="section-small-title">
                                <h3>Listing Details</h3>
                            </div>
                            <!-- section title start -->
                            <!-- client address form -->
                            <div class="client-address-form">
                                <form action="/create_listing" method="POST">
                                    <!-- input title -->
                                    <p>Title</p>
                                    <input type="text" name="truck_name">
                                    <!-- input brand -->
                                    <p>Brand</p>
                                    <input type="text" name="truck_brand">
                                    <!-- input mileage -->
                                    <p>Mileage</p>
                                    <input type="text" name="truck_km">
                                    <!-- input fuel type -->
                                    <p>Fuel Type</p>
                                    <input type="text" name="truck_fuel">
                                    <!-- input drive -->
                                    <p>Drive</p>
                                    <input type="text" name="truck_drive">
                                    <!-- input color -->
                                    <p>Color</p>
                                    <input type="text" name="truck_color">
                                    <!-- add pictures -->
                                    <p>Pictures</p>
                                    <input type="text">
                                    <p>Length of Time (Days) </p>
                                    <div class="cart-quantity cart-column">
                                        <input id="carto" class="cart-quantity-input" type="number" value="1">
                                    </div>
                                    <p></p>

                                    <div class="shop-items">
                                                    <% items.merch.forEach(function(item){ %>
                                                        <div class="shop-item" data-item-id="<%= item.id %>">
                                                            <span class="shop-item-title" style="width:0px;height:0px;font-size:0px;"><%= item.name %></span>
                                                            <img class="shop-item-image" src="<%= item.imgName %>" style="width:0px;height:0px;">
                                                            <div class="shop-item-details">
                                                                <span class="shop-item-price" style="width:0px;height:0px;font-size:0px;">$<%= item.price / 100 %></span>

                                                            </div>
                                                        </div>
                                                    <% }) %>
                                                </div>

                                    <div id ="cart-items" class="cart-items">
                                     </div>

                                    <button class="btn btn-primary shop-item-button btn-purchase" type="button">Post Listing</button>

                                    <div class="cart-total">
                                        <strong class="cart-total-title">Total</strong>
                                        <span class="cart-total-price">$5</span>
                                    </div>

                                </form>

                            </div>
                        </div>
                        <!-- post listing end -->
                    </div>
                </div>
