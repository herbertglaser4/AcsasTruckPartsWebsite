                <div class="row">
                    <!-- listing slider start -->
                    
                    <!-- TODO: FIX QUERY FOR NEWLY ADDED LISTING DATE IN DB -->
                    
                    <div class="listing-slider owl-preview-2">
                        <% listings.forEach(function(truck){ %>
                            <div class="col-md-12">
                                <div class="single-list-shop" data-item-id="<%= truck.id %>">
                                    <!-- single listing start -->
                                    <div class="listing-details">
                                        <!-- listing img start -->
                                        <div class="listing-img">
                                            <a href="/trucks/<%= truck.id%>">
                                                <img src="../Front End/img/product/<%=truck.imgName %>.jpg" style="width:212.5px;height:159px;" alt="">
                                                <span class="post-date">
                                                    <%=truck.date %>
                                                </span>
                                            </a>
                                        </div>
                                        <!-- listing img end -->
                                        <!-- listing content start -->
                                        <div class="listing-info">
                                            <!-- post title -->
                                            <a href="/trucks/<%= truck.id%>" class="listing-title"> <%= truck.name %> </a>
                                            <!-- post info -->
                                            <p class="post-info">
                                                <span> <i class="icofont icofont-user"></i> <%= truck.email %></span>
                                            </p>
                                            <!-- post summary -->
                                            <p class="post-summary">
                                                <%= truck.blah %>
                                            </p>
                                            <!-- read post button -->
                                            <a href="/trucks/<%= truck.id%>" class="read-post"> See Listing </a>
                                        </div>
                                        <!-- listing content end -->
                                    </div>
                                <!-- single listing start -->
                                </div>
                            </div>
                        <% }) %>
                    </div>
                    <!-- listing slider end -->
                </div>
            </div>
