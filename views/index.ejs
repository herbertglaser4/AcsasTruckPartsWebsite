        <!-- shop area start -->
        <div class="shop-area section-pad">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <!-- section title start -->
                        <div class="section-heading text-center">
                            <h3>Featured Parts</h3>
                        </div>
                        <!-- section title end -->
                    </div>
                </div>
                <div class="shop-tab-area">
                    <!-- Shop tabs -->
                    <ul class="shop-tab" role="tablist">
                        <li role="presentation" class="active"><a href="#tab1" aria-controls="tab1" role="tab" data-toggle="tab">NEW ARRIVALS</a></li>
                        <li role="presentation"><a href="#tab3" aria-controls="tab3" role="tab" data-toggle="tab">BEST SELLING</a></li>
                    </ul>
                    <!-- Shop Tab panes -->
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane fade in active" id="tab1">
                            
                            <div class="row">
                                <% news.forEach(function(item){ %>
                                    <div class="col-md-3 col-sm-6">
                                        <div class="single-list-shop" data-item-id="<%= item.id %>">
                                        <!-- single product start -->
                                            <div class="single-product hover-style text-center">
                                                <div class="product-img">
                                                    <!-- product-img -->
                                                    <a href="/parts/<%= item.id%>" class="main-img">
                                                        <img src="../Front End/img/product/<%=item.imgName %>.jpg" style="width:212.5px;height:159px;">
                                                    </a>
                                                </div>
                                                <!-- product-info start -->
                                                <div class="product-info text-center">
                                                    <div class="product-name">
                                                        <a href="/parts/<%= item.id%>"><%= item.name %></a>
                                                    </div>
                                                    <div class="product-price">
                                                        <p>$<%= item.price / 100 %></p>
                                                    </div>
                                                </div>
                                                <!-- product-info start -->
                                            </div>
                                            <!-- single product end -->
                                        </div>
                                    </div>
                                <% }) %>
                            </div>
                        </div>
                    
                        
                        <div role="tabpanel" class="tab-pane fade" id="tab3">
                            <div class="row">
                                <% pops.forEach(function(item){ %>
