        <!-- shop area start -->
        <div class="shop-area pages section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <form action="/searchTrucks" method="post">
                                <input type="text" placeholder="Search.." name="searchBar">
                                <button type="submit" value="search">Go</button>
                            </form>
                        <!-- shop bar start -->
                        <div class="shop-bar">
                            <!-- shop tab bar start -->
                            <div class="shop-nav width-33 pull-left">
                                <ul class="shop-navigation" role="tablist">
                                    <li role="presentation" class="active gird">
                                        <a href="#gird" aria-controls="gird" role="tab" data-toggle="tab">
                                            <i class="fa fa-th"></i>
                                        </a>
                                    </li>
                                    <li role="presentation" class="list">
                                        <a href="#list" aria-controls="list" role="tab" data-toggle="tab">
                                            <i class="fa fa-bars"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <!-- shop tab bar end -->
                            <!-- shop sort start -->
                            <div class="sort-by width-33 pull-left text-center">
                                <label>Sort By: </label>
                                <form action="/sortTrucks" method="post">
                                <select name="sortlist">
                                    <option value="name" selected >Name</option>
                                    <option value="new">Newest</option>
                                    <option value="mile">Mileage</option>
                                </select>
                                <button type='submit'>Sort</button>
                                </form>
                            </div>
                            <!-- shop sort end -->
                            <!-- shop show product start -->
                            <div class="limit-product width-33 pull-left text-right">
                                <label>Show: </label>
                                <select name="show">
                                    <option value="#" selected>9</option>
                                    <option value="#">12</option>
                                    <option value="#">24</option>
                                    <option value="#">36</option>
                                </select>
                            </div>
                            <!-- shop show product end -->
                        </div>
                        <!-- shop bar end -->
                    </div>
                </div>
                <!-- jsonjson -->
                <div class="row">
                    <!-- shop tab content start -->
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane fade in active" id="list">
                            <!-- list shop start -->
                            <div class="list-shop">
                                <!-- single list shop start -->
                                <% items.forEach(function(item){ %>
                                <div class="single-list-shop" data-item-id="<%= item.id %>">
                                    <div class="col-md-3 col-sm-5">
                                        <!-- product img start -->
                                        <div class="product-img">
                                            <a href="/trucks/<%= item.id%>"><img src="../img/product/<%=item.imgName %>.jpg" style="width:250px;height:250px;" class="shop-item-image"></a>
                                        </div>
                                        <!-- product img end -->
                                    </div>
                                    <div class="col-md-9 col-sm-7">
                                        <div class="single-product">
                                            <div class="product-name">
                                                <a href="/trucks/<%= item.id%>" class="shop-item-title"><%= item.name %></a>
                                            </div>
                                            
                                            <div class="price-rating">
                                                <div class="truck-details pull-left">
                                                    <p><b>Brand</b>: <%= item.brand %> &nbsp;
                                                        <b>Fuel Type</b>: <%= item.fuel %> &nbsp;
                                                        <b>Drive</b>: <%= item.drive %></p>
                                                </div>
                                            </div>
                                            <div class="product-text">
                                                <p><%= item.blah %></p>
                                            </div>
                                            <!-- cart plus minus start -->
                                            <div class="contact-info">
                                                <label class="shop-item-price"><%= item.email %></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% }) %>
                            </div>
                            <!-- list shop end -->
                        </div>
                    </div>
                    <!-- shop tab content end -->
                </div>
